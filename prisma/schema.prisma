generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model WorkoutPlan {
  order_id    String @id
  customer_id String
  level       String
  goal        String
  facilities  String
  details     String
  content     String
}