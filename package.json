{
  "name": "fitplan",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "npm run prisma:migrate",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "stripe:webhook": "stripe listen --forward-to http://localhost:5173/",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "pg:start": "brew services start postgresql@14",
    "pg:stop": "brew services stop postgresql@14"
  },
  "devDependencies": {
    "@picocss/pico": "^2.0.6",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-vercel": "^5.2.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "crypto-es": "^2.1.0",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "markdown-it": "^14.1.0",
    "mdsvex": "^0.11.0",
    "nodemailer": "^6.9.13",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "sass": "^1.75.0",
    "stripe": "^15.1.0",
    "svelte": "4.2.14",
    "svelte-check": "^3.6.0",
    "svelte-time": "^0.9.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "url-safe-base64": "^1.3.0",
    "vite": "^5.0.3",
    "vite-plugin-markdown": "^2.2.0",
    "vitest": "^1.2.0"
  },
  "type": "module",
  "dependencies": {
    "@langchain/community": "^0.0.48",
    "@langchain/groq": "^0.0.8",
    "@prisma/client": "^5.12.1",
    "@types/nodemailer": "^6.4.14",
    "@vercel/analytics": "^1.2.2",
    "@vercel/flags": "^2.3.0",
    "@vercel/postgres": "^0.5.1",
    "@vercel/speed-insights": "^1.0.10",
    "@vercel/toolbar": "^0.1.14",
    "langchain": "^0.1.33",
    "mdsvex": "^0.11.0",
    "postmark": "^4.0.2",
    "prisma": "^5.12.1",
    "svelte-email": "^0.0.4",
    "svelte-sonner": "^0.3.22"
  }
}
